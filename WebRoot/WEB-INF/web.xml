<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
	<display-name></display-name>
	<servlet>
		<servlet-name>ListServlet</servlet-name>
		<servlet-class>com.uestc.testmybatis.servlet.ListServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DeleteOneMessageServlet</servlet-name>
		<servlet-class>com.uestc.testmybatis.servlet.DeleteOneMessageServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DeleteBatchServlet</servlet-name>
		<servlet-class>com.uestc.testmybatis.servlet.DeleteBatchMessageServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>InitTalkServlet</servlet-name>
		<servlet-class>com.uestc.testmybatis.servlet.InitTalkServlet</servlet-class>
	</servlet>


	<servlet-mapping>
		<servlet-name>ListServlet</servlet-name>
		<url-pattern>/list.action</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DeleteOneMessageServlet</servlet-name>
		<url-pattern>/deleteone.action</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DeleteBatchServlet</servlet-name>
		<url-pattern>/deletbatch.action</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>InitTalkServlet</servlet-name>
		<url-pattern>/talk.action</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
